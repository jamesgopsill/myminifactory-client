{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@jamesgopsill/myminifactory-client",
	"version": "0.0.1",
	"description": "A client for working with the MyMiniFactory API.",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"keywords": [
		"Eiger API",
		"Markforged"
	],
	"author": "James Gopsill",
	"license": "MIT",
	"private": false,
	"files": [
		"dist/**/*"
	],
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"engines": {
		"node": ">=19.0.0"
	},
	"engineStrict": true,
	"scripts": {
		"test": "jest",
		"build:dist": "rm -rf dist && tsc --project tsconfig.json && tsc --project tsconfig.cjs.json && node create-package-jsons.mjs",
		"build:docs": "typedoc",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@tsconfig/node18-strictest": "^1.0.0",
		"@tsconfig/node18-strictest-esm": "^1.0.1",
		"@types/jest": "^29.4.0",
		"@types/node": "^18.13.0",
		"jest": "^29.4.1",
		"prettier": "^2.8.3",
		"ts-jest": "^29.0.5",
		"typedoc": "^0.23.24",
		"typescript": "^4.9.4"
	}
}
